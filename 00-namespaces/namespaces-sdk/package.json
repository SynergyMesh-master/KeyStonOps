{
  "name": "namespace-sdk",
  "version": "1.0.0",
  "description": "A machine-native, auditable platform integration layer for MCP tool wrapping",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write &quot;src/**/*.ts&quot;",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "sdk",
    "api-wrapper",
    "integration",
    "github",
    "cloudflare",
    "openai",
    "google",
    "audit",
    "governance",
    "machine-native"
  ],
  "author": "NinjaTech AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninjatech-ai/machine-native-ops.git",
    "directory": "00-namespaces/namespaces-sdk"
  },
  "bugs": {
    "url": "https://github.com/ninjatech-ai/machine-native-ops/issues"
  },
  "homepage": "https://github.com/ninjatech-ai/machine-native-ops/tree/main/00-namespaces/namespaces-sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@octokit/rest": "^20.0.0"
  },
  "peerDependenciesMeta": {
    "@octokit/rest": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
